<table class='table table-striped table-hover'>
	<thead>
		<th>Title</th>
    <th>Date Created</th>
    <th>Date Modified</th>
  </thead>

  <tbody>
    <% @sketches.each do |sketch| %>
    <tr>
      <td><%= link_to sketch.title, sketch_path(sketch) %></td>
      <td><%= sketch.created_at %></td>
      <td><%= sketch.updated_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>